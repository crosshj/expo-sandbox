{
  "expo": {
    "name": "bob",
    "description": "Day Trading Bob",
    "slug": "bob",
    "scheme": "bob",
    "privacy": "unlisted",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#1a4e63",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a4e63"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.crosshj.bob"
    },
    "androidStatusBar": {
      "backgroundColor": "#1a4e63",
      "barStyle": "light-content"
    }
  }
}
